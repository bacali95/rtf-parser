{
  "name": "@bacali/rtf-parser",
  "version": "1.0.0",
  "description": "This is a general RTF parser.  It takes a text stream and produces a document object representing the parsed document.  In and of itself, this isn't super useful but it's the building block for other tools to convert RTF into other formats.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "private": false,
  "dependencies": {
    "iconv-lite": "^0.6.2",
    "readable-stream": "^3.6.0"
  },
  "devDependencies": {
    "@types/readable-stream": "^2.3.9",
    "prettier": "^2.0.5",
    "standard": "^14.3.4",
    "standard-version": "*",
    "tap": "^14.10.7",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.9.7",
    "weallbehave": "*",
    "weallcontribute": "*"
  },
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "build": "tsc",
    "test": "node test/test.js"
  },
  "keywords": [
    "rtf"
  ],
  "author": "Rebecca Turner <me@re-becca.org> (http://re-becca.org/)",
  "license": "ISC",
  "files": [
    "index.js",
    "rtf-interpreter.js",
    "rtf-parser.js",
    "rtf-group.js",
    "rtf-document.js",
    "rtf-paragraph.js",
    "rtf-span.js"
  ],
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bacali/rtf-parser.git"
  },
  "bugs": {
    "url": "https://github.com/bacali/rtf-parser/issues"
  },
  "homepage": "https://npmjs.com/package/@bacali/rtf-parser"
}
